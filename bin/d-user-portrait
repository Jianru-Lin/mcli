#!/usr/bin/env node

var m = require('../index')
var vstr = require('vstr')
var argv = require('yargs').usage('Usage: d-user-portrait -n <name> -r <doc_rev>')
                           .option('name', {alias: 'n', demand: true, type: 'string'})
                           .option('rev', {alias: 'r', demand: true, type: 'string'})
                           .argv


var opt = {
    name: argv.name,
    rev: argv.rev
}
m.delete_user_portrait(opt, require('../cmd_cb'))
