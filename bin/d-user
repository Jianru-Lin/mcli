#!/usr/bin/env node

var m = require('../index')
var vstr = require('vstr')
var argv = require('yargs').usage('Usage: d-user -n <name> -r <rev>')
                           .option('name', {alias: 'n', demand: true, type: 'string'})
                           .option('rev', {alias: 'r', demand: true, type: 'string'})
                           .argv

var opt = {
    name: argv.n,
    rev: argv.rev
}
m.delete_user(opt)