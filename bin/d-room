#!/usr/bin/env node

var m = require('../index')
var vstr = require('vstr')
var argv = require('yargs').usage('Usage: d-user -i <id> -r <rev>')
                           .option('id', {alias: 'i', demand: true, type: 'string'})
                           .option('rev', {alias: 'r', demand: true, type: 'string'})
                           .argv

var opt = {
    id: argv.id,
    rev: argv.rev
}
m.delete_room(opt, require('../cmd_cb'))