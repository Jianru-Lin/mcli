#!/usr/bin/env node

var m = require('../index')
var vstr = require('vstr')
var argv = require('yargs').usage('Usage: c-user -n <name> -p <password>')
                           .option('name', {alias: 'n', demand: true, type: 'string'})
                           .option('password', {alias: 'p', demand: true, type: 'string'})
                           .argv

var opt = {
    name: argv.name,
    password: argv.password
}
m.create_user(opt)