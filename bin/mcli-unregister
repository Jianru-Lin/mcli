#!/usr/bin/env node

var m = require('../index')
var vstr = require('vstr')
var argv = require('yargs').usage('Usage: mcli-unregister -n <name>')
                           .option('name', {alias: 'n', demand: true, type: 'string'})
                           .argv

var opt = {
    name: argv.n
}
m.unregister(opt)